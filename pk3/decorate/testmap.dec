actor DAKKTEST_InstantDeathTester
{
    Radius 10
    Height 28
    
    Health 1
    Scale 0.33
    
    +SOLID
    +SHOOTABLE
    
    States
    {
      Spawn:
        SKEL A 1 A_SetAngle(angle + 45)
        loop
      
      Death:
        SKEL A 0 A_SpawnItemEx("DAKKTEST_InstantDeathFX")
