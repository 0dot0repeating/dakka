      Steam:
        TNT1 A 1 A_SpawnItemEx("MinigunSteam", 0,0,0, velx/2, vely/2, velz/2, angle, SXF_ABSOLUTEANGLE | SXF_ABSOLUTEVELOCITY)
        stop

      Death:
        TNT1 A 0 A_JumpIf(waterlevel > 1, "Steam")
        TNT1 A 0 A_JumpIf(CallACS("Dakka_MinigunFireStyle") == 1, "OldFireDeath")
        goto NewFireDeath

      FireDone:
        TNT1 A 1
        stop


      OldFire:
        TNT1 A 0 bright A_JumpIf(CallACS("Dakka_LessEffects"), "OldFireLoop_LE")
        goto OldFireLoop

      OldFireLoop_LE:
        TNT1 A 0 A_JumpIf(waterlevel > 1, "Steam")
        TNT1 A 0 A_SetUserVar("user_t", user_t+1)
        TNT1 A 0 A_SpawnItemEx("MinigunOldFlame_LE", 0, 0, 0, velx/2,vely/2,(velz/2) + frandom(0,2), 0, SXF_ABSOLUTEPOSITION | SXF_ABSOLUTEVELOCITY)
        TNT1 A 1 A_JumpIf(user_t > 15, "FireDone")
        loop

      OldFireLoop:
        TNT1 A 0 A_JumpIf(waterlevel > 1, "Steam")
        TNT1 A 0 A_SetUserVar("user_t", user_t+1)
        TNT1 A 0 A_SpawnItemEx("MinigunOldFlame_LE", 0,            0,            0,         velx/2,vely/2,(velz/2) + frandom(0,2), 0, SXF_ABSOLUTEPOSITION | SXF_ABSOLUTEVELOCITY)
        TNT1 A 0 A_SpawnItemEx("MinigunOldFlame",   (velx/3.0)*1, (vely/3.0)*1, (velz/3)*1, velx/2,vely/2,(velz/2) + frandom(0,2), 0, SXF_ABSOLUTEPOSITION | SXF_ABSOLUTEVELOCITY)
