        DMNG A 1 A_WeaponReady(WRF_NOSECONDARY)
        goto Ready


      Spawn:
        DMNG Z -1
        loop
    }
}

actor MinigunFlameCounter: Counter {}

actor MinigunFlame
{
    Radius 2
    Height 2

    Projectile
    Speed 25
    Damage (0)
    DamageType "Fire"

    +RIPPER
    +PAINLESS
    +NOBOSSRIP
    +DONTREFLECT
    +MTHRUSPECIES
    +BLOODLESSIMPACT
    +NODAMAGETHRUST
    +HITTRACER

    RenderStyle Add
    Scale 0.2
    Alpha 0.5

    Obituary "$DAKKA_MP_MINIGUN_FLAME"

    var int user_counter;
    var int user_spawned;

    Decal "FireScorch"

    States
    {
      Spawn:
        TNT1 E 0 bright
        TNT1 E 0 bright ACS_NamedExecuteWithResult("Dakka_InheritVelocity", AAPTR_TARGET, 80, 0, 30)
        TNT1 E 0 bright A_ScaleVelocity(random(90, 110) / 100.0)
        TNT1 E 0 bright A_SpawnItemEx("MinigunFlameV", 0,0,0, velx,vely,velz, angle, SXF_TRANSFERPOINTERS|SXF_ABSOLUTEPOSITION|SXF_ABSOLUTEANGLE|SXF_ABSOLUTEVELOCITY|SXF_CLIENTSIDE)
        TNT1 E 0 bright A_SetUserVar("user_counter", 15)
        TNT1 E 0 bright A_SetUserVar("user_spawned", true)
        goto Spawn2

      Spawn2:
        TNT1 A 0 A_JumpIf(waterlevel > 1, "SteamEnd")
        TNT1 A 0 ACS_NamedExecuteWithResult("Dakka_StartMinigunBurn", 32, 2, 105, false)
        TNT1 A 1 A_SetUserVar("user_counter", user_counter - 1)
        TNT1 A 0 A_JumpIf(user_counter > 0, "Spawn2")
        goto EndItAll

      SteamEnd:
        TNT1 A 1 A_Explode(1, 64, false, false, 64)
        stop

      EndItAll:
        TNT1 A 1
        stop

      Death:
        TNT1 A 0 A_JumpIf(user_spawned, 2)
        TNT1 A 0 A_SpawnItemEx("MinigunFlameV_Dead", 0,0,0, 0,0,0, angle, SXF_ABSOLUTEANGLE | SXF_CLIENTSIDE)
        TNT1 A 1 ACS_NamedExecuteWithResult("Dakka_StartMinigunBurn", 32, 4, 105, true)
        stop
    }
}


