        TNT1 A 1 A_JumpIf(user_t > 15, "FireDone")
        loop

      OldFireDeath:
        TNT1 A 0 A_JumpIf(user_t < 15, 1)
        goto FireDone

        TNT1 A 0 A_SpawnItemEx("MinigunOldFlame_LE", 0,0,0, 0,0,0, 0, SXF_ABSOLUTEPOSITION)
        TNT1 A 0 A_SpawnItemEx("MinigunImpactFlame", frandom(-15,15), 0, frandom(-15,15), frandom(-2,2),0,frandom(0,2), random(0,360))
        TNT1 A 1 A_SetUserVar("user_t", user_t + 4)
        loop


      NewFire:
        TNT1 A 0 bright A_JumpIf(CallACS("Dakka_LessEffects"), "NewFireLoop_LE")
        goto NewFireLoop

      NewFireLoop:
        TNT1 A 0 A_JumpIf(waterlevel > 1, "Steam")
        TNT1 A 0 A_SetUserVar("user_t", user_t+1)
        TNT1 A 0 A_JumpIf(user_t > 16, "FireDone")
        TNT1 A 0 A_JumpIf(user_t > 13, "Fire6")
        TNT1 A 0 A_JumpIf(user_t > 10, "Fire5")
        TNT1 A 0 A_JumpIf(user_t > 7,  "Fire4")
        TNT1 A 0 A_JumpIf(user_t > 4,  "Fire3")
        TNT1 A 0 A_JumpIf(user_t > 2,  "Fire2")
        goto Fire1

      Fire1:
        TNT1 A 1 A_SpawnItemEx("MinigunNewFlame1", frandom(-3,3) - (velx/2.0), -vely/2.0, -velz/2.0, (velx/2)+frandom(-1.5, 1.5), (vely/2)+frandom(-0.4, 0.4), (velz/2)+frandom(-0.2, 0.2), 0, SXF_ABSOLUTEPOSITION|SXF_ABSOLUTEVELOCITY|SXF_ABSOLUTEANGLE, 0)
        goto NewFireLoop

      Fire2:
        TNT1 A 1 A_SpawnItemEx("MinigunNewFlame2", frandom(-3,3) - (velx/2.0), -vely/2.0, -velz/2.0, (velx/2)+frandom(-1.5, 1.5), (vely/2)+frandom(-0.6, 0.6), (velz/2)+frandom(-0.3, 0.3), 0, SXF_ABSOLUTEPOSITION|SXF_ABSOLUTEVELOCITY|SXF_ABSOLUTEANGLE, 24)
        goto NewFireLoop

      Fire3:
        TNT1 A 1 A_SpawnItemEx("MinigunNewFlame3", frandom(-3,3) - (velx/2.0), -vely/2.0, -velz/2.0, (velx/2)+frandom(-1.5, 1.5), (vely/2)+frandom(-0.8, 0.8), (velz/2)+frandom(-0.4, 0.4), 0, SXF_ABSOLUTEPOSITION|SXF_ABSOLUTEVELOCITY|SXF_ABSOLUTEANGLE, 48)
