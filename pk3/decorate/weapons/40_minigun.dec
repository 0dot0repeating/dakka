actor MinigunFlameV
{
    Radius 2
    Height 2

    Projectile
    Damage (0)

    +RIPPER
    +PAINLESS
    +NOBOSSRIP
    +DONTREFLECT
    +MTHRUSPECIES
    +BLOODLESSIMPACT
    +CLIENTSIDEONLY

    var int user_t;

    States
    {
      Spawn:
        TNT1 A 0
        TNT1 A 0 A_ScaleVelocity(random(90, 110) / 100.0)
