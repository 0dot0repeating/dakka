actor DakkaSSG_Grenade
{
    Radius 3
    Height 6
    Speed 55

    Projectile
    Damage (0)

    +FORCEXYBILLBOARD
    +THRUACTORS
    +HITTRACER

    Scale 0.35
    
    +BOUNCEONFLOORS
    +BOUNCEONCEILINGS
    +BOUNCEONWALLS
    BounceFactor 1

    ProjectileKickback 100
    Obituary "$DAKKA_MP_SUPERSHOTGUN_GYRO"

    var int user_spawnvelx;
    var int user_spawnvely;
    var int user_spawnvelz;

    var int user_velx;
    var int user_vely;
    var int user_velz;
    var int user_t;

    States
    {
      Spawn:
        DGRN A 0
        DGRN A 0 ACS_NamedExecuteWithResult("Dakka_ProjUpdate")
        DGRN A 0 bright A_SpawnItemEx("ShotNadeTrail",  0,         0,         0,        0,0,0, 0, SXF_ABSOLUTEPOSITION)
        DGRN A 0 bright A_SpawnItemEx("ShotNadeTrail", -1*momx/2, -1*momy/2, -1*momz/2, 0,0,0, 0, SXF_ABSOLUTEPOSITION)
        DGRN A 1 A_JumpIf(user_t > 2, "Detonate")
        loop
      
      Detonate:
        TNT1 A 0 A_Stop
        goto Death

      Death:
        DGRN A 0 bright ACS_NamedExecuteWithResult("Dakka_ProjDeathUpdate")
        DGRN A 0 bright A_Stop
        DGRN A 0 bright A_NoGravity
        DGRN A 0 bright A_ChangeFlag("PAINLESS",       random(0, 2) != 0)
        DGRN A 0 bright A_ChangeFlag("FORCERADIUSDMG", random(0, 1) == 0)
        DGRN A 0 bright A_Explode(args[0], args[1], false, false, args[2])
        TNT1 A 2 A_SpawnItemEx("SSGAltExplosion_Client")
        stop
    }
}


// args are in order: explosive damage, explosion radius, explosion full damage radius
actor DakkaSSG_Grenade1: DakkaSSG_Grenade { Args 18, 80,  48 }
actor DakkaSSG_Grenade2: DakkaSSG_Grenade { Args 14, 96,  56 }
actor DakkaSSG_Grenade3: DakkaSSG_Grenade { Args 10, 112, 64 }
actor DakkaSSG_Grenade4: DakkaSSG_Grenade { Args 7,  128, 72 }




actor SSGAltExplosion_Client
{
    +NOINTERACTION
    +CLIENTSIDEONLY
    +FORCEXYBILLBOARD

    RenderStyle Add
    Scale 0.5

    States
    {
      Spawn:
        MISL B 0 bright
        MISL B 0 A_Jump(96, 2)
        MISL B 0 A_PlaySound("dakka/ssgaltexplode")
        goto ParticleSpawn

      ParticleSpawn:
        MISL BBB 0 A_SpawnItemEx("GrenadeExplosionSpark", frandom(-6, 6), frandom(-1, 1), frandom( 2,  6), frandom(4, 12), frandom(-3, 3), frandom(0, 10), frandom(0, 360))
        goto SmokeSpawn

      SmokeSpawn:
        MISL B 0 bright A_JumpIf(CallACS("Dakka_LessEffects") == true, "NoSmoke")
        MISL BBB 0 A_SpawnItemEx("ImpalerBigTrailParticle",   frandom(-6, 6), frandom(-1, 1), frandom(-4, 4), frandom(-3, 3), 0,              frandom(-3, 3), frandom(0, 360))

      NoSmoke:
        DMIS GHIJ 2 bright
        stop
    }
}