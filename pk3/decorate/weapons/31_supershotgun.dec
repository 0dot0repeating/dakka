        TNT1 A 0 A_SpawnItemEx("DakkaSSG_Grenade2", 0,0,0, user_spawnvelx/65536.0, user_spawnvely/65536.0, user_spawnvelz/65536.0, angle, SXF_ABSOLUTEANGLE | SXF_ABSOLUTEVELOCITY | SXF_TRANSFERPOINTERS)
        TNT1 A 0 ACS_NamedExecuteWithResult("Dakka_CalcProjAngle", 62, 1860, 740)
        TNT1 A 0 A_SpawnItemEx("DakkaSSG_Grenade2", 0,0,0, user_spawnvelx/65536.0, user_spawnvely/65536.0, user_spawnvelz/65536.0, angle, SXF_ABSOLUTEANGLE | SXF_ABSOLUTEVELOCITY | SXF_TRANSFERPOINTERS)
        TNT1 A 0 ACS_NamedExecuteWithResult("Dakka_CalcProjAngle", 73, 1860, 740)
        TNT1 A 0 A_SpawnItemEx("DakkaSSG_Grenade2", 0,0,0, user_spawnvelx/65536.0, user_spawnvely/65536.0, user_spawnvelz/65536.0, angle, SXF_ABSOLUTEANGLE | SXF_ABSOLUTEVELOCITY | SXF_TRANSFERPOINTERS)
        
        TNT1 A 0 ACS_NamedExecuteWithResult("Dakka_CalcProjAngle", 84, 1860, 740)
