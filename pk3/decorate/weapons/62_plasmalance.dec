        PLNC D 1 offset( 1, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge3")
        PLNC D 1 offset(-1, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge3")
        PLNC E 1 offset( 0, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge3")
        PLNC E 1 offset( 1, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge3")
        PLNC E 1 offset( 0, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge3")
        PLNC B 1 offset(-1, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge3")
        
        PLNC B 0 A_JumpIfNoAmmo("Altfire_Charge3")
        PLNC B 0 A_FireCustomMissile("AmmoWaster")
        PLNC B 0 A_AlertMonsters(448)
        PLNC B 1 offset( 0, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge4")
        PLNC B 1 offset( 2, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge4")
        PLNC C 1 offset( 1, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge4")
        PLNC C 1 offset( 0, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge4")
        PLNC C 1 offset(-1, 33) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge4")
        PLNC D 1 offset(-2, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge4")
        PLNC D 1 offset(-2, 33) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge4")
        PLNC D 1 offset( 1, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge4")
        
        PLNC B 0 A_JumpIfNoAmmo("Altfire_Charge4")
        PLNC B 0 A_FireCustomMissile("AmmoWaster")
        PLNC B 0 A_AlertMonsters(576)
        PLNC E 1 offset( 0, 33) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge5")
        PLNC E 1 offset(-2, 33) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge5")
