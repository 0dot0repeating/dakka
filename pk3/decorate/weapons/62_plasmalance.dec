      AltFire:
        PLNC F 0 A_JumpIfNoAmmo("AltNo")
        PLNC F 0 A_PlaySound("arc/altcharge",      CHAN_WEAPON)
        PLNC F 0 A_GunFlash("Charging")
        
        PLNC A 1 offset(0, 32)
        PLNC A 0 A_AlertMonsters(64)
        PLNC A 0 A_FireCustomMissile("AmmoWaster")
        PLNC AAABBBB 1 A_JumpIfInventory("AltReleased", 1, "AltFire_Charge0")
        
        PLNC B 0 A_JumpIfNoAmmo("Altfire_Charge0")
        PLNC B 0 A_FireCustomMissile("AmmoWaster")
        PLNC B 0 A_AlertMonsters(128)
        PLNC CCCCDDDD 1 A_JumpIfInventory("AltReleased", 1, "AltFire_Charge1")
        
        PLNC B 0 A_JumpIfNoAmmo("Altfire_Charge1")
        PLNC B 0 A_FireCustomMissile("AmmoWaster")
        PLNC B 0 A_AlertMonsters(224)
        PLNC E 1 offset(-1, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge2")
        PLNC E 1 offset( 0, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge2")
        PLNC E 1 offset( 0, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge2")
        PLNC B 1 offset( 1, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge2")
        PLNC B 1 offset( 0, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge2")
        PLNC B 1 offset( 0, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge2")
