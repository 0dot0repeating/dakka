        PLNC C 1 offset( 3, 33) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge6")
        PLNC D 1 offset(-1, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge6")
