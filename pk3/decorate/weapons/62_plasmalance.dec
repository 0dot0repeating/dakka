        PLNC F 1        offset(0, 33) A_JumpIfInventory("MainFired", 1, "FireRight")
        PLNC F 0                      A_WeaponReady(WRF_NOPRIMARY | WRF_NOBOB)
        PLNC F 1        offset(0, 33) A_JumpIfInventory("MainFired", 1, "FireRight")
        PLNC F 0                      A_WeaponReady(WRF_NOPRIMARY | WRF_NOBOB)
        PLNC F 1        offset(0, 32) A_JumpIfInventory("MainFired", 1, "FireRight")
