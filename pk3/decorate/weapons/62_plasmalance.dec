        PLNC G 0                      A_AlertMonsters
        PLNC G 0                      ACS_NamedExecuteWithResult("Dakka_Switcharoo", 13, 0)
        PLNC G 0                      ACS_NamedExecuteAlways("Dakka_Recoil", 0, -115, 1, 8)
