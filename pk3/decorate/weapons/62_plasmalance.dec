        PLNC E 1 offset( 0, 32) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge6")
        PLNC B 1 offset(-1, 33) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge6")
        PLNC B 1 offset(-3, 33) A_JumpIfInventory("AltReleased", 1, "AltFire_Charge6")
