actor Pickup_Chainsaw: ClassBasedPickup replaces Chainsaw
{
    States
    {
      Spawn:
        TNT1 A 0 A_SetUserVar("user_index", 0)
        TNT1 A 0 A_SetUserVar("user_dropped", 0)
        goto DropCheck

      Unknown:
        CSAW A -1
        stop

      Dakka:
        SCPR Z -1
        stop

      Pickup:
        TNT1 A 0 A_JumpIf(ACS_ExecuteWithResult(PICKUP_PICKUP, 0, 0) == 1, "PickedUp")
        goto NotPickedUp
    }
}
